<Window x:Class="WPFGame1.Pause"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStyle="None"
        AllowsTransparency="True"
        WindowStartupLocation="CenterScreen"
        Background="Transparent"
        Title="Pausee" Height="120" Width="120" KeyDown="Window_KeyDown">
    <Grid Height="80" Width="80">
        <Rectangle Fill="WhiteSmoke" Height="50" Margin="22,14,45,0" Name="rectangle1" Stroke="Black" VerticalAlignment="Top" Panel.ZIndex="1" />
        <Rectangle Fill="WhiteSmoke" Height="50"  Margin="45,14,22,0" Name="rectangle2" Stroke="Black" VerticalAlignment="Top" Panel.ZIndex="1" />
        <Ellipse Fill="Black" Height="70" Name="ellipse1" Stroke="Black" Width="70" />
    </Grid>
</Window>
